{
  "name": "teraapi/teraapi",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-mysqli": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Core\\": "Core/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
